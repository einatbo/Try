/*---------------------------------------------------------------------------------------------------------*/
/* Include PC-Lint Out-Of-The-Box Configurations                                                           */
/*---------------------------------------------------------------------------------------------------------*/
-i"C:\Lint\lnt"

/*---------------------------------------------------------------------------------------------------------*/
/* Include Compiler specifig PC-Lint Configuration (CR16C)                                                 */
/*---------------------------------------------------------------------------------------------------------*/
SWC_LINT\co-cr16c.lnt

/*---------------------------------------------------------------------------------------------------------*/
/* Project inclues and definitions                                                                         */
/*---------------------------------------------------------------------------------------------------------*/
 -i"Bridge"
 -i"MT_COMMON"
 -i"MT_HI"
 -i"MT_HI\MT_HI_USB"
 -i"MT_TRANSPORT"
 -i"MT_TRANSPORT\MT_TRANSPORT_HID" 
 -i"MT_TRANSPORT\MT_TRANSPORT_NMTP" 

 -dMAX_TOUCH_POINTS=5
 -dNMTP_BRIDGE
 
/*---------------------------------------------------------------------------------------------------------*/
/* Include SWC_DEFS PC-Lint Configuration                                                                  */
/*---------------------------------------------------------------------------------------------------------*/
 -i"SWC_DEFS"
 swc-defs.lnt

/*---------------------------------------------------------------------------------------------------------*/
/* Include SWC_HAL PC-Lint Configuration                                                                   */
/*---------------------------------------------------------------------------------------------------------*/
 -i"SWC_HAL"
 -dCHIP_NAME=npce79nx -dEXTERNAL_INCLUDE_FILE=chip_patch.h
 swc-hal.lnt

/*---------------------------------------------------------------------------------------------------------*/
/* Multi Touch Project-Specific PC-Lint Configuration                                                      */
/*---------------------------------------------------------------------------------------------------------*/
proj-opt.lnt
